{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceGroupName": {
      "value": "rg-ado-template-#{environment_stage_abbreviation}#"
    },
    "resourceLocation": {
      "value": "westeurope"
    },
    "tags": {
      "value": {
        "LandingZone": "#{landing_zone_name}#",
        "Workload": "#{solution_name}#",
        "Environment": "#{environment_stage_abbreviation}#",
        "OperationsTeam": "#{operations_team}#",
        "Purpose": "This resource group is the #{environment_stage_abbreviation}# stage for the #{solution_name}# workload"
      }
    }
  }
}
